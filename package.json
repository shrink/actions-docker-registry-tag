    "eslint": "^7.17.0",
    "eslint-plugin-github": "^4.1.1",
    "eslint-plugin-jest": "^24.1.3",
    "jest": "^26.6.3",
    "jest-circus": "^26.6.3",
    "js-yaml": "^3.14.0",
    "prettier": "2.2.1",
